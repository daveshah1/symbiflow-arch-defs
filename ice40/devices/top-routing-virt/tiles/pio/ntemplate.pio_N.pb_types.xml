<!-- set: ai sw=1 ts=1 sta et -->
<pb_type name="BLK_TL-PIO_{N}" capacity="2" xmlns:xi="http://www.w3.org/2001/XInclude">
 <!-- SB_IO inputs   -->
 <output name="io_D_IN" num_pins="2" equivalent="false"/>

 <!-- SB_IO outputs -->
 <input  name="io_D_OUT" num_pins="2" equivalent="false"/>

 <!-- Control signals -->
 <input  name="io_OUT_ENB" num_pins="1" equivalent="false"/>

 <!--
 <input  name="io_global_cen"    num_pins="1" equivalent="false"/>
 <clock  name="io_global_inclk"  num_pins="1" equivalent="false"/>
 <clock  name="io_global_outclk" num_pins="1" equivalent="false"/>
 <input  name="io_global_latch"  num_pins="1" equivalent="false"/>
 -->

 <!-- IO operating as an output -->
 <mode name="PAD_IS_OUTPUT">
  <pb_type name="PAD" num_pb="1">
   <input name="D_OUT" num_pins="2" equivalent="false"/>
   <pb_type name="output" blif_model=".output" num_pb="1">
    <input name="outpad" num_pins="1"/>
   </pb_type>
   <interconnect>
    <mux name="D_OUT" input="PAD.D_OUT[0] PAD.D_OUT[1]" output="output.outpad"/>
   </interconnect>
  </pb_type>

  <interconnect>
   <!-- SB_IO outputs -->
   <direct name="io_D_OUT[0]" input="BLK_TL-PIO_{N}.io_D_OUT[0]" output="PAD.D_OUT[0]" />
   <direct name="io_D_OUT[1]" input="BLK_TL-PIO_{N}.io_D_OUT[1]" output="PAD.D_OUT[1]" />
  </interconnect>
 </mode>

 <!-- IO operating as an input -->
 <mode name="PAD_IS_INPUT">
  <pb_type name="PAD" num_pb="1">
   <output name="D_IN" num_pins="2" equivalent="false"/>
   <pb_type name="input" blif_model=".input" num_pb="1">
    <output name="inpad" num_pins="1"/>
   </pb_type>
   <interconnect>
    <mux name="D_IN_0" input="input.inpad" output="PAD.D_IN[0]"/>
    <mux name="D_IN_1" input="input.inpad" output="PAD.D_IN[1]"/>
   </interconnect>
  </pb_type>

  <interconnect>
   <!-- SB_IO inputs -->
   <direct name="io_D_IN[0]" input="PAD.D_IN[0]" output="BLK_TL-PIO_{N}.io_D_IN[0]" />
   <direct name="io_D_IN[1]" input="PAD.D_IN[1]" output="BLK_TL-PIO_{N}.io_D_IN[1]" />
  </interconnect>
 </mode>

 <pinlocations pattern="custom">
  <loc side="right" xoffset="0" yoffset="0">
   BLK_TL-PIO_{N}.io_D_IN
   BLK_TL-PIO_{N}.io_D_OUT
   BLK_TL-PIO_{N}.io_OUT_ENB
  </loc>
 </pinlocations>
 <fc in_type="frac" in_val="1.0" out_type="frac" out_val="1.0" />
</pb_type>
